        android:layout_toEndOf="@+id/iv_lucas_parker"
        android:fontFamily="@font/poppins_semibold"
        android:text="@string/lucas_parker"
        android:textColor="@color/black"
        android:textSize="@dimen/_22sp" />

    <TextView
        android:id="@+id/tv_time_lucas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_lucas_parker"
        android:layout_alignStart="@+id/tv_vina_gracia"
        android:layout_alignEnd="@+id/tv_vina_gracia"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/time_lucas" />

    <View
        android:id="@+id/view2"
        android:layout_width="@dimen/_380dp"
        android:layout_height="@dimen/_1dp"
        android:layout_below="@id/iv_lucas_parker"
        android:layout_alignStart="@+id/iv_background_global_rank"
        android:layout_alignEnd="@+id/iv_background_global_rank"
        android:layout_marginStart="@dimen/_5dp"
        android:layout_marginTop="@dimen/_5dp"
        android:layout_marginEnd="@dimen/_10dp"
        android:background="@android:color/darker_gray" />

    <TextView
        android:id="@+id/tv_no3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/view3"
        android:layout_below="@id/view2"
        android:layout_marginTop="@dimen/_20dp"
        android:layout_marginStart="@dimen/_36dp"
        android:layout_marginBottom="@dimen/_20dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="@string/_3"
        android:textColor="#CD7F32"
        android:textSize="@dimen/_22sp" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/iv_you"
        android:layout_width="@dimen/_60dp"
        android:layout_height="@dimen/_60dp"
        android:layout_below="@id/view2"
        android:layout_marginStart="24dp"
        android:layout_marginTop="@dimen/_10dp"
        android:layout_toEndOf="@id/tv_no1"
