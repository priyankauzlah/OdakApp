        android:layout_marginEnd="@dimen/_10dp"
        android:background="@android:color/darker_gray" />

    <TextView
        android:id="@+id/tv_no7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/view7"
        android:layout_below="@id/view6"
        android:layout_marginTop="@dimen/_20dp"
        android:layout_marginBottom="@dimen/_20dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="@string/_7"
        android:layout_marginStart="@dimen/_36dp"
        android:textColor="#C4C4C4"
        android:textSize="@dimen/_22sp" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/iv_taylor_lashae"
        android:layout_width="@dimen/_60dp"
        android:layout_height="@dimen/_60dp"
        android:layout_below="@id/view6"
        android:layout_marginStart="24dp"
        android:layout_marginTop="@dimen/_10dp"
        android:layout_toEndOf="@id/tv_no1"
        android:src="@drawable/taylor_lashae" />

    <TextView
        android:id="@+id/tv_taylor_lashae"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
